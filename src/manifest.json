{
  "name": "Chrome extension boilerplate",
  "description": "A boilerplate for building Chrome extensions",
  "version": "0.1",
  "manifest_version": 3,
  "permissions": [
    "activeTab",
    "scripting",
    "storage",
    "tabs"
  ],
  "options_page": "options.html",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "64": "images/dark/extension-icon64.png"
    }
  }
}